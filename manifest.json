{
  "manifest_version": 2,
  "name": "AutoPixie",
  "description": "DOM automation. Set custom scripts to the push of a button",
  "version": "1.0.0",
  "background" : {
    "scripts" : ["background.js"]
  },
  "browser_action": {
   "default_icon": "icon-16-bw.png"
  },
  "icons": { "16": "icon-16-bw.png",
    "48": "icon-48.png",
   "128": "icon-128.png" },
  "permissions": ["tabs", "<all_urls>","storage"]
}